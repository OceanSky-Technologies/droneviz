{
  "name": "droneviz",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "dev": "pnpm build:backend && echo \"Run 'pnpm run:backend' in parallel to start the backend.\" && vite dev",
    "run:backend": "pnpm build:backend && .\\backend\\start.bat",
    "build": "pnpm build:app && pnpm build:backend",
    "build:app": "vite build",
    "build:backend": "bash ./backend/download.bash",
    "preview": "npx dotenvx run --env='NODE_ENV=production' -- concurrently \"vite preview\" \"npx tsx dist/backend/server\"",
    "test": "vitest run --dom",
    "clean": "del-cli --force dist dev-dist coverage src-tauri/target",
    "check-types": "tsc --noEmit",
    "lint": "eslint",
    "format": "prettier . --write",
    "format-check": "prettier . --check",
    "depcheck": "depcheck"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.14.0",
    "@eslint/js": "^9.10.0",
    "@tauri-apps/cli": "2.0.0-rc.13",
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^22.5.4",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vitest/browser": "^2.0.5",
    "@vitest/coverage-istanbul": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "del-cli": "^5.1.0",
    "depcheck": "^1.4.7",
    "eslint": "^9.10.0",
    "eslint-plugin-jsdoc": "^50.2.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.28.0",
    "happy-dom": "^15.7.3",
    "playwright": "^1.47.0",
    "postcss": "^8.4.45",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^13.0.0",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.4.0",
    "vite": "^5.4.3",
    "vite-plugin-pwa": "^0.20.5",
    "vite-plugin-static-copy": "^1.0.6",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5",
    "workbox-window": "^7.1.0"
  },
  "dependencies": {
    "@tauri-apps/api": "2.0.0-rc.5",
    "cesium": "^1.121.1",
    "concurrently": "^9.0.1",
    "cssnano": "^7.0.6",
    "egm96-universal": "^1.1.0",
    "globals": "^15.9.0",
    "primeicons": "^7.0.0",
    "primevue": "^4.0.5",
    "tailwindcss-primeui": "^0.3.4",
    "vue": "^3.5.3"
  },
  "packageManager": "pnpm@9.10.0+sha512.73a29afa36a0d092ece5271de5177ecbf8318d454ecd701343131b8ebc0c1a91c487da46ab77c8e596d6acf1461e3594ced4becedf8921b074fbd8653ed7051c"
}
