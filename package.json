{
  "name": "droneviz",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "dev": "pnpm build:backend && echo \"Run 'pnpm run:backend' in parallel to start the backend.\" && vite dev",
    "run:backend": "pnpm build:backend && .\\backend\\start.bat",
    "build": "pnpm build:app && pnpm build:backend",
    "build:app": "vite build",
    "build:backend": "bash ./backend/download.bash",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "preview": "npx dotenvx run --env='NODE_ENV=production' -- concurrently \"vite preview\" \"npx tsx dist/backend/server\"",
    "test": "vitest run --dom",
    "clean": "del-cli --force dist dev-dist coverage src-tauri/target",
    "check-types": "tsc --noEmit",
    "lint": "eslint",
    "format": "prettier . --write",
    "format-check": "prettier . --check",
    "depcheck": "depcheck"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.16.1",
    "@eslint/js": "^9.12.0",
    "@tauri-apps/cli": "2.0.2",
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^22.7.5",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/browser": "^2.1.2",
    "@vitest/coverage-istanbul": "^2.1.2",
    "@vitest/ui": "^2.1.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "del-cli": "^5.1.0",
    "depcheck": "^1.4.7",
    "eslint": "^9.12.0",
    "eslint-plugin-jsdoc": "^50.3.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.28.0",
    "happy-dom": "^15.7.4",
    "playwright": "^1.48.0",
    "postcss": "^8.4.47",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^13.0.0",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.1",
    "vite": "^5.4.8",
    "vite-plugin-pwa": "^0.20.5",
    "vite-plugin-static-copy": "^1.0.6",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.2",
    "workbox-window": "^7.1.0"
  },
  "dependencies": {
    "@primevue/themes": "^4.1.0",
    "@tauri-apps/api": "2.0.0-rc.5",
    "cesium": "^1.122.0",
    "concurrently": "^9.0.1",
    "cssnano": "^7.0.6",
    "egm96-universal": "^1.1.0",
    "globals": "^15.10.0",
    "mavlink-ts": "link:mavlink-ts",
    "primeicons": "^7.0.0",
    "primevue": "^4.1.0",
    "tailwindcss-primeui": "^0.3.4",
    "vue": "^3.5.11"
  },
  "packageManager": "pnpm@9.12.1+sha512.e5a7e52a4183a02d5931057f7a0dbff9d5e9ce3161e33fa68ae392125b79282a8a8a470a51dfc8a0ed86221442eb2fb57019b0990ed24fab519bf0e1bc5ccfc4"
}
