{
  "productName": "droneviz",
  "version": "1.0.0",
  "identifier": "com.oceansky.droneviz",
  "build": {
    "beforeDevCommand": "pnpm dlnode && pnpm build:server && pnpm dev",
    "beforeBuildCommand": "pnpm generate && pnpm dlnode && pnpm build:server",
    "devUrl": "http://127.0.0.1:3000",
    "frontendDist": "../.tauri-build/dist"
  },
  "app": {
    "windows": [
      {
        "title": "droneviz",
        "label": "main",
        "url": "/",
        "width": 1280,
        "height": 720,
        "resizable": true,
        "additionalBrowserArgs": "--disable-web-security"
      },
      {
        "title": "VideoAI",
        "label": "video-ai",
        "url": "/video-ai",
        "width": 640,
        "height": 480,
        "visible": false,
        "additionalBrowserArgs": "--disable-web-security"
      }
    ],
    "security": {
      "csp": null,
      "headers": {
        "Access-Control-Allow-Headers": "*",
        "Access-Control-Allow-Methods": "*",
        "Access-Control-Allow-Credentials": "true",
        "Access-Control-Expose-Headers": "*"

      }
    }
  },
  "bundle": {
    "active": true,
    "targets": ["deb", "rpm", "appimage", "nsis", "app", "dmg"],
    "icon": [
      "../public/oceansky-logo30x30.png",
      "../public/oceansky-logo100x100.png",
      "../public/oceansky-logo.png",
      "../public/oceansky-logo.icns",
      "../public/oceansky-logo.ico"
    ],
    "externalBin": ["../.tauri-build/node/droneviz-node"],
    "resources": ["../.output/server","../.output/public/_nuxt"]
  },
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json"
}
