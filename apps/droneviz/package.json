{
  "name": "droneviz",
  "productName": "droneviz",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "keywords": [],
  "author": {
    "name": "Martin Schlodinski"
  },
  "description": "Droneviz",
  "main": "dist/src/index.js",
  "scripts": {
    "copy-files": "shx mkdir -p ./dist/src && shx cp -r ./src/css ./dist/src",
    "copy-resources": "shx mkdir -p ./dist/resources && shx cp -r ../../resources ./dist",
    "build": "yarn copy-files && yarn copy-resources && tsc && webpack --config webpack.config.js",
    "watch": "tsc -w",
    "clean": "shx rm -rf dist",
    "start": "webpack serve --config webpack.config.js",
    "start:built": "npx http-server -a localhost -p 8080 dist/"
  },
  "devDependencies": {
    "@types/copyfiles": "^2.4.4",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.0",
    "shx": "^0.3.4",
    "style-loader": "^4.0.0",
    "typescript": "^5.5.3",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "cesium": "^1.119.0"
  }
}
